<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="flyn.androidtransitions">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShareElementActivity1" />
        <activity android:name=".ShareElementActivity2" />
        <activity android:name=".InterpolatorDurationStartDelayActivity1" />
        <activity android:name=".InterpolatorDurationStartDelayActivity2" />
        <activity android:name=".CircularRevealActivity1" />
        <activity android:name=".CircularRevealActivity2" />
        <activity android:name=".PathMotionActivity1" />
        <activity android:name=".PathMotionActivity2" />
    </application>

</manifest>